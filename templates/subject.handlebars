 
    <div id="subject-list" class="row">
        <div class="col-xs-12"> 
        <a href="javascript:void(0)" {{action goBack}} class="btn btn-primary btn-standard"><i class="fa fa-chevron-left"></i> Tillbaka</a>
        </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
      <h1>Ämnesord i Jämda</h1>
       <table class="table table-striped table-hover">
        <tr><th>Ämnesord</th><th>Antal</th><tr>
          {{#each item in subjectArr}}
            <tr><td>{{#link-to 'search' query=item.subject startyear=App.MINYEAR endyear=App.MAXYEAR numberofitems=App.MAXCOUNT rows='10'}}{{item.subject}}{{/link-to}}</td> <td><span class="badge">{{item.count}}</span></td></tr>
          {{/each}}
        </table>
      </div>
    </div>
